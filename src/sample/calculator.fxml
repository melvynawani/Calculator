<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="214.0" prefWidth="294.0">
         <children>
            <TextArea fx:id="textArea" editable="false" layoutX="33.0" layoutY="14.0" prefHeight="57.0" prefWidth="235.0" />
            <GridPane layoutX="33.0" layoutY="97.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button fx:id="button1" mnemonicParsing="false" onAction="#onPressButton1" prefHeight="27.0" prefWidth="26.0" text="1" />
                  <Button fx:id="button2" mnemonicParsing="false" onAction="#onPressButton2" text="2" GridPane.columnIndex="1" />
                  <Button fx:id="button3" mnemonicParsing="false" onAction="#onPressButton3" text="3" GridPane.columnIndex="2" />
                  <Button fx:id="button4" mnemonicParsing="false" onAction="#onPressButton4" text="4" GridPane.columnIndex="3" />
                  <Button fx:id="button5" mnemonicParsing="false" onAction="#onPressButton5" text="5" GridPane.rowIndex="1" />
                  <Button fx:id="button6" mnemonicParsing="false" onAction="#onPressButton6" text="6" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Button fx:id="button7" mnemonicParsing="false" onAction="#onPressButton7" text="7" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Button fx:id="button8" mnemonicParsing="false" onAction="#onPressButton8" text="8" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <Button fx:id="button9" mnemonicParsing="false" onAction="#onPressButton9" text="9" GridPane.rowIndex="2" />
                  <Button fx:id="button0" mnemonicParsing="false" onAction="#onPressButton0" text="0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
            <GridPane layoutX="167.0" layoutY="97.0">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <Button fx:id="buttonAdd" mnemonicParsing="false" onAction="#onPressAddButton" prefHeight="27.0" prefWidth="26.0" text="+" />
                  <Button fx:id="buttonSubtract" mnemonicParsing="false" onAction="#onPressSubtractButton" prefHeight="27.0" prefWidth="24.0" text="-" GridPane.columnIndex="1" />
                  <Button fx:id="buttonEquals" mnemonicParsing="false" onAction="#onPressEqualButton" prefHeight="27.0" prefWidth="49.0" text="=" GridPane.columnSpan="2" GridPane.rowIndex="2" />
                  <Button fx:id="multiplyButton" mnemonicParsing="false" onAction="#onPressMultiplyButton" text="x" GridPane.rowIndex="1" />
                  <Button fx:id="divideButton" mnemonicParsing="false" onAction="#onPressDivideButton" prefHeight="27.0" prefWidth="24.0" text="/" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <Button fx:id="resetButton" layoutX="220.0" layoutY="97.0" mnemonicParsing="false" onAction="#onPressResetButton" prefHeight="81.0" prefWidth="49.0" text="C" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
